{
    "name": "breerly/factory-girl-php",
    "type": "library",
    "description": "Fixture replacement for focused and readable tests - A PHP port of Thoughtbot's Ruby Factory Girl",
    "keywords": [
        "factory girl",
        "fixture factory",
        "fixture replacement",
        "object mother",
        "testing",
        "fixture",
        "tdd"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Grayson Koonce",
            "email": "breerly@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "doctrine/orm": ">=2.2.1",
        "doctrine/dbal": ">=2.2.1",
        "doctrine/common": ">=2.2.1"
    },
    "autoload": {
        "psr-0": {
            "FactoryGirl\\Tests": "tests",
            "FactoryGirl": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    }
}

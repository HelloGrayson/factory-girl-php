{
    "name": "breerly/factory-girl-php",
    "type": "library",
    "description": "Fixture replacement for focused and readable tests - A PHP port of Thoughtbot's Ruby Factory Girl",
    "keywords": [
        "factory girl",
        "fixture factory",
        "fixture replacement",
        "object mother",
        "testing",
        "fixture",
        "tdd"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Grayson Koonce",
            "email": "breerly@gmail.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": "^7.1",
        "doctrine/annotations": "^1.7.0",
        "doctrine/common": "^2.2.1",
        "doctrine/dbal": "^2.2.1",
        "doctrine/orm": "^2.6.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14.0",
        "phpunit/phpunit": "^7.5.1"
    },
    "suggest": {
        "fzaninotto/faker": "For generating fake data in entity definitions"
    },
    "autoload": {
        "psr-4": {
            "FactoryGirl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FactoryGirl\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "cs": "php-cs-fixer fix --diff --verbose"
    }
}
